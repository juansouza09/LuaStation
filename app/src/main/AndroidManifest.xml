<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.luastation">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.LuaStation">
        <activity
            android:name=".HomeActivity"
            android:exported="true" />

        <activity android:name="com.example.luastation.firebase.login.LoginActivity"
            android:exported="true"/>

        <activity android:name="com.example.luastation.firebase.cadastro.EscolhaActivity"
            android:exported="true"/>

        <activity android:name="com.example.luastation.firebase.cadastro.Etapa1Activity"
            android:exported="true"/>

        <activity android:name="com.example.luastation.firebase.cadastro.Etapa1EmpresaActivity"
            android:exported="true"/>

        <activity android:name="com.example.luastation.firebase.cadastro.Etapa2Activity"
            android:exported="true"/>

        <activity android:name="com.example.luastation.firebase.cadastro.Etapa2EmpresaActivity"
            android:exported="true"/>

        <activity android:name="com.example.luastation.firebase.cadastro.Etapa3Activity"
            android:exported="true"/>

        <activity android:name=".menusuperior.MenuActivity"
            android:exported="true"/>

        <activity android:name=".menusuperior.SegurancaActivity"
            android:exported="true"/>

        <activity android:name=".menusuperior.CriarProjetoActivity"
            android:exported="true"/>

        <activity android:name=".menusuperior.CriarProjeto2Activity"
            android:exported="true"/>

        <activity android:name=".menusuperior.PagamentoProjetoActivity"
            android:exported="true"/>

        <activity android:name=".menusuperior.ProjetoAdicionadoActivity"
            android:exported="true"/>

        <activity android:name=".DetalhesActivity"
            android:exported="true"/>

        <activity android:name=".PerfilContratanteActivity"
            android:exported="true"/>

        <activity
            android:name=".SplashActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>